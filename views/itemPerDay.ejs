<% include partials/header %>
    <div class="container">
       
            <form action="/itemperday" method="post">
                <table class="responsive-table" class="centered" class="highlight">
                        <thead>
                                <tr>
                                    <th>Item Name</th>
                                    <th>Quantity</th>
                                </tr>
                              </thead>
                            <tbody>
                                <% for (let i = 0; i < rate.length; ++i) { %>
                                    <tr>
                                        <td class="addtocart"><%= rate[i].dataValues.name %></td>
                                        <td>
                                            <input type="text" name="item[<%= i %>][id]" value="<%= rate[i].dataValues.id %>" hidden=true>
                                            <input type="text" name="item[<%= i %>][qty]" value="0">
                                        </td>
                                        <td>Rs<%= rate[i].dataValues.price %>/<%= rate[i].dataValues.qtype %></td>
                                    </tr>
                                
                                <% } %>
                    </tbody>
                </table>
                <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                        <i class="material-icons right">send</i>
                    </button>
                            
            </form>
       
 </div>
<% include partials/footer %>