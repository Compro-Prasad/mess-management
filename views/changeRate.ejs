<% include partials/header %>
<div class="container">
    <div class="row">
        <form action="/changerate" method="post" class="col s12">
            <div class="row" style="padding-left:20em;padding-right: 20em;">
                    <div class="input-field ">
                        <select name="rate[id]">
                            <% for (let i = 0; i < row.length; ++i) { %>
                                <option value="<%= row[i].dataValues.id %>"><%= row[i].dataValues.name %></option>
                            <% } %>
                        </select>
                    </div>
            </div>
            <div class="row" style="padding-left:20em;padding-right: 20em;">
                    <div class="input-field ">
                        <label for="rate[price]">Item Price</label>
                        <input type="text" name="rate[price]" />
                    </div>
            </div>
            <div class="row" style="padding-left:20em;padding-right: 20em;" align="center">
                <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                        <i class="material-icons right">send</i>
                </button>
            </div>
        </form>
    </div>
</div>  
<% include partials/footer %>
